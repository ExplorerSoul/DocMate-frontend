# .env.example

# Address of the blockchain smart contract used for locking
VITE_LOCK_CONTRACT=your_lock_contract_address_here

# API key for Lighthouse (used for file uploads/IPFS)
VITE_LIGHTHOUSE_API_KEY=your_lighthouse_api_key_here

# URL of your backend server (can be localhost or deployed URL)
VITE_BACKEND_URL=http://localhost:5000
